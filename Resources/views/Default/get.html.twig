{% extends 'VlRadRestBundle:Default:skeleton.html.twig' %}
{% block radrest_content %}
<table>
  {% for key in data|radrest_object_keys %}
    <tr><th>{{ key }}</th><td>{{ attribute(data, key)|radrest_object_stringify }}</td></tr>
  {% endfor %}
</table>
{% include 'VlRadRestBundle:Default:links.html.twig' %}
{% endblock %}
